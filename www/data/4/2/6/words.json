[{"id":1,"name":"Wir haben uns gestern gestritten."},{"id":2,"name":"Mach mich nicht wütend!"},{"id":3,"name":"Lass mich in Ruhe!"},{"id":4,"name":"Halt den Mund!"},{"id":5,"name":"Schrei mich nicht an!"},{"id":6,"name":"Warum tust du mir das an?"},{"id":7,"name":"Ich wollte dich nicht verletzen!"},{"id":8,"name":"Lass uns nicht darüber streiten!"},{"id":9,"name":"Es tut mir leid, was ich gesagt habe."},{"id":10,"name":"Sei mir nicht böse."}]